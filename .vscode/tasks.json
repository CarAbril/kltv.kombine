{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "kombine-build",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"label": "kombine-publish",
			"command": "mkb",
			"type": "process",
			"args": [
				"publish",
				"${workspaceFolder}"
			],
			"problemMatcher": "$msCompile"
		}		
	]
}